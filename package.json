{
  "name": "high-speed-file-processor",
  "version": "1.0.0",
  "description": "Production-ready concurrent file processing system",
  "main": "dist/index.js",
  "scripts": {
    "dev:api": "nodemon",
    "dev:worker": "nodemon --watch src/workers --exec ts-node src/workers/worker.main.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:worker": "node dist/workers/worker.main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:up": "docker compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker compose -f docker/docker-compose.yml down",
    "docker:logs": "docker compose -f docker/docker-compose.yml logs -f",
    "migrate": "ts-node src/db/migrate.ts"
  },
  "dependencies": {
    "bullmq": "^5.1.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^2.4.5",
    "pg": "^8.11.3",
    "sharp": "^0.33.2",
    "tesseract.js": "^5.0.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "license": "MIT"
}
